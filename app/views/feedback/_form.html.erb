<%= form_with url: feedback_index_url, local: true do |form| %>
  <p>
    <%= form.label :email %><br />
    <%= form.email_field :email, autofocus: true, autocomplete: "email", required: true, value: current_user.email %>
  </p>
  <p>
    <%= form.label :message %><br>
    <%= form.text_area :message, required: true %>
  </p>
  <p>
    <%= form.submit value: t('.send') %>
  </p>
<% end %>